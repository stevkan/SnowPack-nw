{
  "name": "fgu-module-maker",
  "version": "0.1.0",
  "private": true,
  "main": "http://localhost:3000",
  "node-remote": "http://localhost:3000",
  "chromium-args": "--enable-spell-checking --remote-debugging-port=9222",
  "keywords": [
    "nwJs",
    "PixiJs",
    "snowpack",
    "React",
    "template",
    "boilerPlate",
    "HMR",
    "node-webkit",
    "nw",
    "game",
    "app",
    "demo"
  ],
  "scripts": {
    "postinstall": "cd ./node_modules/nw  &&  npm install --nwjs_build_type=sdk",
    "start": "concurrently \"npm run start-web\" \"npm run nw\"",
    "start-nw": "concurrently \"npm run build-nw\" \"npm run dev\" \"npm run nw\"",
    "start-web": "snowpack dev --polyfill-node",
    "dev": "snowpack dev --polyfill-node --open none",
    "nw": "nw . --remote-debugging-port=9222 --polyfill-node",
    "build": "npm run build-web &&  npm run build-nw",
    "build-web": "snowpack build",
    "build-nw": "nw build --platforms win32,win64,linux64,osx64 --buildDir build/ dist/",
    "nw-test-dist": "nw ./dist --remote-debugging-port=9222"
  },
  "author": "Jonathan Lepage",
  "license": "MIT",
  "devDependencies": {
    "@types/nw.js": "^0.13.11",
    "commander": "^7.0.0",
    "nw": "^0.50.3",
    "nw-builder": "^3.5.7",
    "snowpack": "^3.0.10"
  },
  "dependencies": {
    "@nozbe/watermelondb": "^0.23.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.33",
    "@types/nw.js": "^0.13.14",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "bootstrap": "^5.1.1",
    "concurrently": "^6.3.0",
    "formik": "^2.2.9",
    "nw-react-scripts": "4.0.4",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-rc.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-transition-group": "^4.4.2",
    "typescript": "^4.4.4",
    "web-vitals": "^1.1.2"
  },
  "bugs": {
    "url": "https://github.com/nwutils/SnowPack-nw/issues"
  },
  "homepage": "https://github.com/nwutils/SnowPack-nw",
  "repository": {
    "type": "git",
    "url": "https://github.com/nwutils/SnowPack-nw.git"
  }
}
